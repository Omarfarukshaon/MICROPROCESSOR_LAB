.MODEL SMALL
.STACK 100h
.DATA
arr db 5 dup(?)                 
txt db 10,13,'largest element is: $'  
.CODE
MAIN PROC
    mov ax, @data
    mov ds, ax

    lea di, arr
    mov cx, 5
input_loop:
    mov ah, 01
    int 21h
    sub al, 30h      
    mov [di], al
    inc di
    loop input_loop

    lea si, arr
    mov bl, [si]     
    mov cx, 4         
find_max:
    inc si
    mov al, [si]
    cmp bl, al
    jae skip
    mov bl, al   
skip:
    loop find_max

    lea dx, txt
    mov ah, 09
    int 21h

    mov dl, bl
    add dl, 30h    
    mov ah, 02
    int 21h
    mov ah, 4Ch
    int 21h
MAIN ENDP
END MAIN
