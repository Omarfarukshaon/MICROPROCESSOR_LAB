.MODEL SMALL
.STACK 100h

.DATA
arr db 5 dup(?)                 
txt db 10,13,'Sorted array is:$'  

.CODE
MAIN PROC
    mov ax, @data
    mov ds, ax
    lea di, arr
    mov cx, 5
input_loop:
    mov ah, 01
    int 21h
    sub al, 30h   
    mov [di], al
    inc di
    loop input_loop

    mov cx, 4   
sort_pass:
    lea si, arr
    mov bx, 4          
sort_compare:
    mov al, [si]
    cmp al, [si+1]
    jbe no_swap
    xchg al, [si+1]
    mov [si], al
no_swap:
    inc si
    dec bx
    jnz sort_compare
    loop sort_pass
    
    lea dx, txt
    mov ah, 09
    int 21h

    lea si, arr
    mov cx, 5
print_loop:
    mov dl, [si]
    add dl, 30h    
    mov ah, 02
    int 21h 
    mov dl,' '
    int 21h
    inc si
    loop print_loop
    
    mov ah, 4Ch
    int 21h
MAIN ENDP
END MAIN
